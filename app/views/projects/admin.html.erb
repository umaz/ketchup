<h1>申請一覧</h1>
<table id="list_fav" class="table table-striped">
  <tr>
    <th>企画番号</th>
    <th>企画名</th>
    <th>企画団体名</th>
    <th>企画種類</th>
  </tr>
  <% @entry.each do |project| %>
  <tr>
    <td><%= link_to project.id, admin_path(project.id), :class => "btn btn-primary" %></td>
    <td><%= project.name %></td>
    <td><%= project.group %></td>
    <td><%= project.kind %></td>
  </tr>
  <% end %>
</table>
